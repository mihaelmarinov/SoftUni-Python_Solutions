strawberry_price = float(input())
bananas_weight = float(input())
orange_weight = float(input())
raspberry_weight = float(input())
strawberry_weight = float(input())


raspberry_price = strawberry_price * 0.50
orange_price = raspberry_price * 0.60
bananas_price = raspberry_price * 0.20

print((strawberry_price * strawberry_weight) + (bananas_price * bananas_weight) + (orange_price * orange_weight) +
      (raspberry_price * raspberry_weight))
